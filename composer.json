{
    "name": "botiroff/gii",
    "description": "Service Repository Pattern generator for laravel.",
    "keywords": [
        "botiroff",
        "gii"
    ],
    "homepage": "https://github.com/nodirjonbotirov/gii",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Botirov N.",
            "email": "unknownnod@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Botiroff\\Gii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Botiroff\\Gii\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botiroff\\Gii\\GiiServiceProvider"
            ],
            "aliases": {
                "Gii": "Botiroff\\Gii\\GiiFacade"
            }
        }
    }
}
